@model IEnumerable<Gasto_Mensual>

<div class="card shadow" id="Tabla1">
    <div class="card-header card-header-primary" id="HeaderTablaInicio1">
        <h4 class="card-title ">Gastos Mensuales</h4>
        <p class="card-category"> Lista de gastos mensuajes a cubrir en warren</p>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-hover">
                <thead class=" text-primary">
                <th>
                    Descripcion
                </th>
                <th>
                    Fecha Generado
                </th>
                <th>
                    Fecha Vencimiento
                </th>
                <th>
                    ID Pago
                </th>

                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @item.Descripcion
                            </td>
                            <td>
                                @item.Fecha_Generado
                            </td>
                            <td>
                                @item.Fecha_Vencimiento
                            </td>
                            <td>
                                @item.Id_pago
                            </td>

                        </tr>
                    }

                </tbody>
            </table>
        </div>
    </div>
</div>
